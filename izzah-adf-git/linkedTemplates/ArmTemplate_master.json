{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "izzah-adf-git"
		},
		"LS_abs_nintendo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_abs_nintendo'"
		},
		"Ls_adls_data_engineering_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Ls_adls_data_engineering_dl'"
		},
		"Ls_adls_data_engineering_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://formula1dl0206.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "izzah-adf-git_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_abs_nintendo_connectionString": {
						"value": "[parameters('LS_abs_nintendo_connectionString')]"
					},
					"Ls_adls_data_engineering_dl_accountKey": {
						"value": "[parameters('Ls_adls_data_engineering_dl_accountKey')]"
					},
					"Ls_adls_data_engineering_dl_properties_typeProperties_url": {
						"value": "[parameters('Ls_adls_data_engineering_dl_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}